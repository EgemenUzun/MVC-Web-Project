@model Project.DataBase.MvcWebUI.Models.UserDetailsViewModel
<ul class="dropdown-menu dropdown-menu-end">
    @if (!String.IsNullOrEmpty(Model.UserName))
    {
        <li>

            <a>Welcome @Model.UserName</a>
        </li>

        <li>
            <a class="link-dark" asp-controller="Account" asp-action="LogOff">Log Out</a>
        </li>
    }

</ul>
